mixin filter-extended(id, title, tabbed, search, anchors, subheadings, ...items)
  .modal.filter-extended(id="filter-extended-" +id class!=attributes.class)
    .modal-header
      h3.pull-left.tight= title
      i.icon.icon-cross.pull-right.hoverable.close-modal

    .modal-content
      if id != "products"
        .row.border-bottom.block-bot-sm
          .col-xs-8
            .filter-extended-anchors
              for letter, i in "#ABCDEFGHIJKLMNOPQRSTUVWXYZ".split("")
                span.anchor-letter= letter

          .col-xs-4
            input.search-input(type="text", placeholder="Search " + title)

        //- if search
        //-   .border-bottom.block-hor-xs
        //-     //- if tabbed
        //-     //-   .filter-extended-tabs.pull-left
        //-     //-     ul.tab-nav
        //-     //-       li.tab-nav-tab.active Top Brands
        //-     //-       li.tab-nav-tab All Brands
        //-     .block-ver-xs.pull-right
        //-       input.search-input(type="text", placeholder="Search " + id)

        .row
          .col-xs-12
            ul.filter-extended-list.clean
              if subheadings
                li.filter-extended-subheading= subheadings
              
              each item in items
                li
                  label(for=item)
                    input.pull-left(type="checkbox" id=item)
                    span= item

      else if id == "products"
        .row.hierarchical-list
          .col-xs-4.border-right
            ul.filter-extended-list.clean
              each item in items
                li
                  label(for=item)
                    input.pull-left(type="checkbox" id=item)
                    span= item
                    i.icon.icon-angle-right.pull-right.tight.text-grey

          .col-xs-4.border-right
            ul.filter-extended-list.clean
              each item in items
                li
                  label(for=item)
                    input.pull-left(type="checkbox" id=item)
                    span= item
                    i.icon.icon-angle-right.pull-right.tight.text-grey
          
          .col-xs-4
            ul.filter-extended-list.clean
                each item in items
                  li
                    label(for=item)
                      input.pull-left(type="checkbox" id=item)
                      span= item

    .modal-footer
      p.tight.block-top-xs.hoverable.pull-left.close-modal Cancel
      .btn.btn-white.pull-right.close-modal Done


