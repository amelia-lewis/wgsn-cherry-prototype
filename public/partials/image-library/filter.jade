mixin filter(id, search, anchors, ...items)
  .filter.border-right.bg-grey(id="filter-" +id)
    if search
      .filter-search.block-sm.border-bottom
        .search-input
          input(type="text", placeholder="Search " + id)
          span.icon.border: i.fa.fa-search

    if anchors
      .filter-anchors.block-top-sm.block-hor-sm.block-bot-xs.text-center
        for letter, i in "ABCDEFGHIJKLMNOPQRSTUVWXYZ".split("")
          span.anchor-letter.text-bold.hoverable.block-inline= letter

    .filter-list.block-sm
      ul.clean
        each item in items
          li.hoverable
            .util-checkbox.fa.fa-square-o
            span= item

    .filter-bottom-row.text-right.block-sm.border-top
      i.fa.fa-repeat.block-inline.hoverable
      p.tight.block-inline.hoverable Reset filter

